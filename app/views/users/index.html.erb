<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "#{user.user_profile.profile.first_name} #{user.user_profile.profile.last_name}", user %>
    </p>
  <% end %>
</div>

